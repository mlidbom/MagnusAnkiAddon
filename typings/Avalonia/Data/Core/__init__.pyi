import typing, abc

class IPropertyInfo(typing.Protocol):
    @property
    def CanGet(self) -> bool: ...
    @property
    def CanSet(self) -> bool: ...
    @property
    def Name(self) -> str: ...
    @property
    def PropertyType(self) -> typing.Type[typing.Any]: ...
    @abc.abstractmethod
    def Get(self, target: typing.Any) -> typing.Any: ...
    @abc.abstractmethod
    def Set(self, target: typing.Any, value: typing.Any) -> None: ...

