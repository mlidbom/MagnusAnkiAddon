class WellKnownFolder(typing.SupportsInt):
    @typing.overload
    def __init__(self, value : int) -> None: ...
    @typing.overload
    def __init__(self, value : int, force_if_true: bool) -> None: ...
    def __int__(self) -> int: ...
    
    # Values:
    Desktop : WellKnownFolder # 0
    Documents : WellKnownFolder # 1
    Downloads : WellKnownFolder # 2
    Music : WellKnownFolder # 3
    Pictures : WellKnownFolder # 4
    Videos : WellKnownFolder # 5

