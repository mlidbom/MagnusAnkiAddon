class ILauncher(typing.Protocol):
    @abc.abstractmethod
    def LaunchFileAsync(self, storageItem: IStorageItem) -> Task_1[bool]: ...
    @abc.abstractmethod
    def LaunchUriAsync(self, uri: Uri) -> Task_1[bool]: ...


class IStorageBookmarkFile(IStorageBookmarkItem, IStorageFile, typing.Protocol):
    pass


class IStorageBookmarkFolder(IStorageBookmarkItem, IStorageFolder, typing.Protocol):
    pass


class IStorageBookmarkItem(IStorageItem, typing.Protocol):
    @abc.abstractmethod
    def ReleaseBookmarkAsync(self) -> Task: ...


class IStorageFile(IStorageItem, typing.Protocol):
    @abc.abstractmethod
    def OpenReadAsync(self) -> Task_1[Stream]: ...
    @abc.abstractmethod
    def OpenWriteAsync(self) -> Task_1[Stream]: ...


class IStorageFolder(IStorageItem, typing.Protocol):
    @abc.abstractmethod
    def CreateFileAsync(self, name: str) -> Task_1[IStorageFile]: ...
    @abc.abstractmethod
    def CreateFolderAsync(self, name: str) -> Task_1[IStorageFolder]: ...
    @abc.abstractmethod
    def GetItemsAsync(self) -> IAsyncEnumerable_1[IStorageItem]: ...


class IStorageItem(IDisposable, typing.Protocol):
    @property
    def CanBookmark(self) -> bool: ...
    @property
    def Name(self) -> str: ...
    @property
    def Path(self) -> Uri: ...
    @abc.abstractmethod
    def DeleteAsync(self) -> Task: ...
    @abc.abstractmethod
    def GetBasicPropertiesAsync(self) -> Task_1[StorageItemProperties]: ...
    @abc.abstractmethod
    def GetParentAsync(self) -> Task_1[IStorageFolder]: ...
    @abc.abstractmethod
    def MoveAsync(self, destination: IStorageFolder) -> Task_1[IStorageItem]: ...
    @abc.abstractmethod
    def SaveBookmarkAsync(self) -> Task_1[str]: ...


class IStorageProvider(typing.Protocol):
    @property
    def CanOpen(self) -> bool: ...
    @property
    def CanPickFolder(self) -> bool: ...
    @property
    def CanSave(self) -> bool: ...
    @abc.abstractmethod
    def OpenFileBookmarkAsync(self, bookmark: str) -> Task_1[IStorageBookmarkFile]: ...
    @abc.abstractmethod
    def OpenFilePickerAsync(self, options: FilePickerOpenOptions) -> Task_1[IReadOnlyList_1[IStorageFile]]: ...
    @abc.abstractmethod
    def OpenFolderBookmarkAsync(self, bookmark: str) -> Task_1[IStorageBookmarkFolder]: ...
    @abc.abstractmethod
    def OpenFolderPickerAsync(self, options: FolderPickerOpenOptions) -> Task_1[IReadOnlyList_1[IStorageFolder]]: ...
    @abc.abstractmethod
    def SaveFilePickerAsync(self, options: FilePickerSaveOptions) -> Task_1[IStorageFile]: ...
    @abc.abstractmethod
    def TryGetFileFromPathAsync(self, filePath: Uri) -> Task_1[IStorageFile]: ...
    @abc.abstractmethod
    def TryGetFolderFromPathAsync(self, folderPath: Uri) -> Task_1[IStorageFolder]: ...
    @abc.abstractmethod
    def TryGetWellKnownFolderAsync(self, wellKnownFolder: WellKnownFolder) -> Task_1[IStorageFolder]: ...


